# Supabase
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key
SUPABASE_STORAGE_BUCKET=listing-images

# Telegram (get API credentials from https://my.telegram.org/apps)
TELEGRAM_API_ID=123456
TELEGRAM_API_HASH=your_telegram_api_hash
TELEGRAM_CHANNEL=@channel_username
# Generate session string by running: npm run telegram:auth
TELEGRAM_SESSION_STRING=

# OpenAI (for extraction service)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini

# Extraction
EXTRACT_VERSION=v1

# Remote sync server (for Vercel button -> VPS trigger)
SYNC_SERVER_PORT=8787
SYNC_API_TOKEN=change_me_to_long_random_secret
